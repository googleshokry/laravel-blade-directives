{
    "name": "googleshokry/laravel-blade-directives",
    "description": "Handy Blade directives",
    "keywords": [
        "googleshokry",
        "laravel-blade-directives"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "shokry mohamed",
            "email": "sms.shokry.mohamed@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "GoogleShokry\\BladeDirectives\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoogleShokry\\BladeDirectives\\Test\\": "tests",
            "GoogleShokry\\BladeDirectives\\Test\\App\\": "tests/laravel/app"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoogleShokry\\BladeDirectives\\BladeDirectivesServiceProvider"
            ]
        }
    }
}
